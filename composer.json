{
  "name": "mijnkantoor/belastingdienst",
  "description": "Calculate dutch tax objects",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Impaqt Software",
      "email": "info@mijnkantoorapp.nl"
    }
  ],
  "require": {
    "php": "^8.3",
    "nesbot/carbon": "^3.8"
  },
  "require-dev": {
    "phpunit/phpunit": "^12.0",
    "mockery/mockery": "^1.6",
    "phpstan/phpstan": "^2.1",
    "phpro/grumphp": "^2.12",
    "squizlabs/php_codesniffer": "^3.12",
    "friendsofphp/php-cs-fixer": "^3.73"
  },
  "autoload": {
    "psr-4": {
      "Mijnkantoor\\Belastingdienst\\": "src/"
    }
  },
  "autoload-dev": {
    "classmap": [
      "tests/"
    ]
  },
  "config": {
    "allow-plugins": {
      "phpro/grumphp": true
    }
  }
}
